               <representation mediaType="application/xml">
                  <doc title="List Domain Details, no Records, no Subdomains Request: XML">
                     <xsdxt:code href="../samples/dns-list-domain-details-no-records-no-subdomains_request.xml"/>
                  </doc>
               </representation>
               <representation mediaType="application/json">
                  <doc title="List Domain Details, no Records, no Subdomains Request: JSON">
