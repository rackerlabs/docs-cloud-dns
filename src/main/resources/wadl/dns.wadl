   <method id="PUT_d1e9994" name="PUT">
            <wadl:doc xmlns="http://docbook.org/ns/docbook" title="Modify PTR Records">
               <para role="shortdesc">Modify one or more PTR records associated with a Rackspace Cloud device.</para>
               <note xmlns:xlink="http://www.w3.org/1999/xlink"
               xmlns:xi="http://www.w3.org/2001/XInclude"
               xmlns:svg="http://www.w3.org/2000/svg"
               xmlns:m="http://www.w3.org/1998/Math/MathML"
               xmlns:html="http://www.w3.org/1999/xhtml">
