                  </doc>
               </representation>
               <representation mediaType="application/json">
                  <doc title="List Domain Details with Records and Subdomains Request: JSON">
                     <xsdxt:code href="../samples/dns-list-domain-details-with-records-and-subdomains_request.json"/>
